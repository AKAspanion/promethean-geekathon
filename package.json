{
  "name": "hackathon-2",
  "version": "0.0.1",
  "description": "All code for hackathon",
  "scripts": {
    "dev:backend": "sh backend/start.sh --prefix backend",
    "dev:frontend": "yarn --cwd frontend dev",
    "start:mock-server": "yarn --cwd --prefix mock-server",
    "start:mock-server-dashboard": "yarn --cwd --prefix mock-server-dashboard",
    "start:mock": "concurrently -n mock,dashboard -c blue,magenta \"npm run start:mock-server\" \"npm run start:mock-server-dashboard\"",
    "dev:mock-server": "npm run dev --prefix mock-server",
    "dev:mock-server-dashboard": "npm run dev --prefix mock-server-dashboard",
    "dev:mock": "concurrently -n mock,dashboard -c blue,magenta \"yarn --cwd mock-server dev\" \"yarn --cwd mock-server-dashboard dev\""
  },
  "homepage": "https://github.com/AKAspanion/hackathon-poc-2#readme",
  "bugs": {
    "url": "https://github.com/AKAspanion/hackathon-poc-2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AKAspanion/hackathon-poc-2.git"
  },
  "license": "ISC",
  "author": "AKASpanion",
  "type": "commonjs",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
