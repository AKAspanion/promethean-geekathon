{
  "name": "mock-server",
  "version": "1.0.0",
  "description": "Extensible mock server with dynamic endpoints and PostgreSQL storage",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "tsx src/server.ts",
    "db:migrate": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "mock",
    "api",
    "postgres",
    "crud"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.6",
    "prisma": "^7.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.19"
  }
}
